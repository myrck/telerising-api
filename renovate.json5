{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:best-practices',
    ':gitSignOff',
  ],
  automergeStrategy: 'squash',
  customManagers: [
    {
      customType: 'regex',
      managerFilePatterns: [
        '/^Dockerfile$/',
      ],
      matchStrings: [
        'ARG\\sTELERISING_API_VERSION="(?<currentValue>.*?)"\\n',
      ],
      depNameTemplate: 'sunsettrack4/telerising-api',
      datasourceTemplate: 'github-releases',
    },
  ],
}
